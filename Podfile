  pod 'RxCocoa', '~> 3.0'
  pod 'RxSwift', '~> 3.0'
  pod 'Swinject', '2.0.0-beta.2'

  target 'LeanPokerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LeanPokerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
